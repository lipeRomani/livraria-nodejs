<html>
    <body>
        
        <h1>Lista de produtos</h1>
        
        <hr />
        <table border="1">
            <tr>
                <td>Id</td>
                <td>Titulo</td>
                <td>Descrição</td>
                <td>Valor</td>
            </tr>
            <% for(var i = 0 ; i < lista.length ; i++){%>
            <tr>
                <td><%= lista[i].id %></td>
                <td><%= lista[i].titulo %></td>
                <td><%= lista[i].descricao %></td>
                <td><%= lista[i].preco %></td>
            </tr>
            <% } %>
           
        </table>
    
    </body>
</html>